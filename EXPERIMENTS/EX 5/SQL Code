-- Create BOOK table
CREATE TABLE BOOK (
    ACCNO INT AUTO_INCREMENT PRIMARY KEY,
    TITLE VARCHAR(255) NOT NULL,
    AUTHOR VARCHAR(255),
    PUBLISHER VARCHAR(255),
    EDITION VARCHAR(100),
    PRICE DECIMAL(10, 2)
);

-- Create MEMBER table
CREATE TABLE MEMBER (
    MID INT AUTO_INCREMENT PRIMARY KEY,
    MNAME VARCHAR(255),
    BRANCH VARCHAR(100)
);

-- Create FINE table
CREATE TABLE FINE (
    MID INT,
    FINE_DATE DATE,
    AMOUNT DECIMAL(10, 2),
    FOREIGN KEY (MID) REFERENCES MEMBER(MID)
);
